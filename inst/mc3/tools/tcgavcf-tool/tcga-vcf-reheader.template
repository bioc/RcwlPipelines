#####
## If generating JSON for multiple samples, replace '</full/file/path>', '<string>', or 'null' with '<>', then complete tcga-vcf-reheader.sample_list.tab.
## See ./backup_templates/tcga-vcf-reheader.sample_list.template for examples. Be certain to provide a unique identifier under the 'run_uid'
## header to clearly distinguish samples/runs. If *optional* sample-specific values are desired, the appropriate key from this template may
## be used as a column header in tcga-vcf-reheader.sample_list.tab (be careful to maintain tab-delimited format).
#####
indelocator_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to VCF generated by Indelocator [e.g., /my/path/to/indelocator.vcf]
muse_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to VCF generated by MuSe [e.g., /my/path/to/muse.vcf]
mutect_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to VCF generated by MuTect [e.g., /my/path/to/mutect.vcf]
pindel_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to VCF generated by Pindel [e.g., /my/path/to/pindel.vcf]
varscani_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to indel VCF generated by VarScan [e.g., /my/path/to/varscani.vcf]
varscans_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to SNP VCF generated by varscan [e.g., /my/path/to/varscans.vcf]
radia_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to VCF generated by RADIA [e.g., /my/path/to/radia.vcf]
somsniper_vcf: {
    class: File
    path: </full/file/path>             # Required file; replace '</full/file/path>' with full path to VCF generated by SomaticSniper [e.g., /my/path/to/sniper.vcf]
normal_aliquot_name: <string>           # Required string; replace '<string>' with normal sample identifier [e.g., TCGA-AB-2901-11A-01W-0732-08]
tumor_aliquot_name: <string>            # Required string; replace '<string>' with tumor sample identifier [e.g., TCGA-AB-2901-03A-01W-0733-08]
normal_bam_name: <string>               # Required string; replace '<string>' with normal BAM filename [e.g., C317.TCGA-AB-2901-11A-01W-0732-08.4_gdc_realn.bam]
tumor_bam_name:  <string>               # Required string; replace '<string>' with tumor BAM filename [e.g., C317.TCGA-AB-2901-03A-01W-0733-08.4_gdc_realn.bam]
normal_aliquot_uuid: null               # Optional string; GDC aliquot UUID [e.g., 22f3f166-41a7-4058-a315-d72e1d7becd6]
tumor_aliquot_uuid: null                # Optional string; GDC aliquot UUID [e.g., bac6a6d2-8c40-43cd-ab4c-16d3f8a06eb6]
normal_analysis_uuid: null              # Optional string; GDC aliquot UUID [e.g., c404dcfb-e580-4cf2-8fe6-6990ce2ce03a]
tumor_analysis_uuid: null               # Optional string; GDC aliquot UUID [e.g., cf8c9b38-246d-46aa-b557-6c882e438161]
platform: null                          # Optional string; one-word description of sequencing platform [e.g., illumina]
center: null                            # Optional string; one-word description of sequencing center [e.g., OHSU]
